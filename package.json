{
  "name": "selena-ide",
  "private": true,
  "version": "0.0.0",
  "description": "IDE for the Selena language, a textual description of renderable UML sequence diagrams.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf \"dist/*\" && rimraf \"grammar/*.js\"",
    "build-grammar": "lezer-generator grammar/selena.grammar -o grammar/selena.js",
    "dev": "npm run build-grammar && webpack --mode development",
    "prod": "npm run build-grammar && webpack --mode production",
    "start": "npm run build-grammar && webpack serve --no-hot",
    "lint": "eslint --ignore-path .gitignore . && stylelint ./public",
    "lint-fix": "eslint --fix --ignore-path .gitignore . && stylelint --fix ./public",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/selena-ide.git"
  },
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/selena-ide/issues"
  },
  "homepage": "https://github.com/meyfa/selena-ide#readme",
  "dependencies": {
    "@codemirror/basic-setup": "^0.18.2",
    "@codemirror/theme-one-dark": "^0.18.1",
    "lezer": "^0.13.5",
    "selena": "^0.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "copy-webpack-plugin": "^9.0.1",
    "eslint": "^7.31.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "html-webpack-plugin": "^5.3.2",
    "lezer-generator": "^0.13.4",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "webpack": "^5.45.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}

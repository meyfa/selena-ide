{
  "name": "selena-ide",
  "private": true,
  "version": "0.0.0",
  "description": "IDE for the Selena language, a textual description of renderable UML sequence diagrams.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && rimraf \"grammar/*.js\"",
    "build-grammar": "lezer-generator grammar/selena.grammar -o grammar/selena.js",
    "dev": "npm run build-grammar && webpack --mode development",
    "prod": "npm run build-grammar && webpack --mode production",
    "start": "npm run build-grammar && webpack serve --no-hot",
    "lint": "eslint --ignore-path .gitignore . && stylelint --ignore-path .gitignore \"./public/**/*.css\"",
    "lint-fix": "eslint --fix --ignore-path .gitignore . && stylelint --fix --ignore-path .gitignore \"./public/**/*.css\"",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meyfa/selena-ide.git"
  },
  "author": "Fabian Meyer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meyfa/selena-ide/issues"
  },
  "homepage": "https://github.com/meyfa/selena-ide#readme",
  "dependencies": {
    "@codemirror/basic-setup": "0.20.0",
    "@codemirror/theme-one-dark": "0.20.0",
    "@lezer/highlight": "0.16.0",
    "blob-stream": "0.1.3",
    "buffer": "6.0.3",
    "debounce": "1.2.1",
    "eventemitter3": "4.0.7",
    "lezer": "0.13.5",
    "pdfkit": "0.13.0",
    "process": "0.11.10",
    "selena": "0.2.0",
    "stream-browserify": "3.0.0",
    "util": "0.12.4"
  },
  "devDependencies": {
    "@lezer/generator": "0.16.0",
    "@types/blob-stream": "0.1.30",
    "@types/debounce": "1.2.1",
    "@types/pdfkit": "0.12.6",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "copy-webpack-plugin": "10.2.4",
    "eslint": "7.32.0",
    "eslint-config-standard-with-typescript": "21.0.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "html-webpack-plugin": "5.5.0",
    "rimraf": "3.0.2",
    "stylelint": "14.8.5",
    "stylelint-config-standard": "25.0.0",
    "ts-loader": "9.3.0",
    "typescript": "4.7.3",
    "webpack": "5.67.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.7.3"
  }
}
